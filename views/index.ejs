<!DOCTYPE html>
<html lang="en">
  <head>
    <meta charset="UTF-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <a href="/cars">Browse Cars</a>
    <a href="/cars/new">Add New Car</a>
    <a href="/cars/">Back to Cars</a>
    <title>Cars App</title>
  </head>
  <body>
    <h1>Welcome to Cars app</h1>
    <ul>
      <% cars.forEach(car => { %>
        <li>
          <a href="/cars/<%= car._id %>"><%= car.name %></a>
        </li>
      <% }); %>
    </ul>
  </body>
</html>